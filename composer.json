{
    "name": "chalcedonyt/laravel-cos-processor",
    "type": "library",
    "description": "Provides functionality to easily process COS (Cheque Outsourcing Services) files.",
    "keywords": [
        "chalcedonyt",
        "cos",
        "cheque",
        "cheque outsourcing services"
    ],
    "homepage": "https://github.com/chalcedonyt/laravel-cos-processor",
    "license": "MIT",
    "authors": [
        {
            "name": "Timothy Teoh",
            "email": "chalcedonyt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "illuminate/database": "~5.1",
        "illuminate/filesystem": "~5.1",
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Chalcedonyt\\COSProcessor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chalcedonyt\\COSProcessor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
